    "hoist-build": "rm -rf ../../functions/builds/game && cp -R build ../../functions/builds/game"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "graphql": "^14.6.0",
    "merge-graphql-schemas": "^1.7.6"
  }
}
