{
  "name": "@dixit/game",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "watch": "nodemon --exec babel-node --root-mode upward ./src/index.js --ignore=node_modules",
    "build": "babel --root-mode upward -d ./build ./src --ignore 'src/**/*.test.js' --copy-files --no-copy-ignored",
